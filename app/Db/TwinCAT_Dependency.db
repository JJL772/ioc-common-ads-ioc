record(lsi, "$(PREFIX):ProjectInfo:$(DEPENDENCY)") {
    field(DISP, 1)
    field(DTYP, "Soft Channel")
    field(INP, ["$(VERSION) -> $(VENDOR)"])
    field(PINI, "YES")
    field(SIZV, 100)
    info(archive, "monitor 1: VAL")
}

record(lsi, "$(PREFIX):ProjectInfo:$(DEPENDENCY):Vendor") {
    field(DISP, 1)
    field(DTYP, "Soft Channel")
    field(INP, ["$(VENDOR)"])
    field(PINI, "YES")
    field(SIZV, 100)
    info(archive, "monitor 1: VAL")
}
