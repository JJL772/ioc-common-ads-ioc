record(longin,"$(PREFIX):AppInfo:TaskCnt"){
 field(PINI, "1")
 field(TSE, -2)
 field(DTYP, "asynInt32")
 field(INP,  "@asyn($(PORT),0,1)ADSPORT=851/TwinCAT_SystemInfoVarList._AppInfo.TaskCnt?")
 field(SCAN,"I/O Intr")
 info(archive, "monitor 1: VAL")
}

